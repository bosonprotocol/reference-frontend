    "start": "node scripts/start.js",
    "start:local": "env-cmd -f .env.local node scripts/start.js",
    "build": "node scripts/build.js",
    "build:local": "env-cmd -f .env.local node scripts/build.js",
    "app:lint": "eslint \"src/**/*.{js,jsx}\"",
    "app:lint-fix": "eslint --fix \"src/**/*.{js,jsx}\"",
    "app:format": "prettier --list-different \"src/**/*.{js,jsx}\"",
    "app:format-fix": "prettier --write \"src/**/*.{js,jsx}\"",
    "tests:lint": "eslint \"test/**/*.{spec,test}.{js,jsx}\"",
    "tests:lint-fix": "eslint --fix \"test/**/*.{spec,test}.{js,jsx}\"",
    "tests:format": "prettier --list-different \"test/**/*.js\"",
    "tests:format-fix": "prettier --write \"test/**/*.js\"",
    "tests:unit": "node scripts/test.js --watchAll=false",
    "tests:unit:coverage": "npm run tests:unit -- --coverage",
    "tests:coverage:badge": "coverage-badge-creator"
  },
